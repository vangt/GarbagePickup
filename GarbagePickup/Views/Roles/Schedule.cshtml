@model GarbagePickup.Models.ApplicationUser
@{
    ViewBag.Title = "Schedule";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Schedule</h2>

<p>Currently it is set to: @Model.ScheduleId</p>

<div class="form-group">
    @Html.LabelFor(model => model.ScheduleId, new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownListFor(model => model.ScheduleId, new SelectList(Model.scheduleList.Date, "Id", "Name"), "", new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.ScheduleId, "", new { @class = "text-danger" })
    </div>
</div>
